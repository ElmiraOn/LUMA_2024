{
    "manifest_version": 3,
    "name": "Link Extractor",
    "version": "1.0",
    "description": "Extract links from the active tab and send to server",
    "permissions": [
      "tabs",
      "storage"
    ],
    "background": {
      "scripts": ["background.js"]  
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "http://localhost:5000/"
    ]
  }
  